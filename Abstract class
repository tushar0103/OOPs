abstract class car{
	  void show(){
		  System.out.println("Main class method");
	  }
	  abstract void wheel();
}
class bmw extends car{
	   
	void drive() {
		System.out.println("Sub class method call drive()");
	}
	void wheel() {
		System.out.println("4 wheel in bmw");
	}
}
public class abst {
	public static void main(String[] a)
	{
		car obj=new bmw();
		obj.show();
	}

}
